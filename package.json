{
  "name": "@elastic/monaco-esql",
  "version": "3.1.12",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "description": "Monaco editor Monarch language syntax definitions for ES|QL",
  "main": "lib/index.js",
  "files": [
    "LICENSE",
    "lib/"
  ],
  "scripts": {
    "format": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "test": "echo \"no tests...\"",
    "clean": "npx rimraf@5.0.5 lib",
    "build:es2020": "tsc --project tsconfig.build.json --module commonjs --target es2020 --outDir lib",
    "build": "yarn build:es2020",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "release": "npx release-it --ci"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elastic/monaco-esql.git"
  },
  "keywords": [
    "esql",
    "es|ql",
    "elastic",
    "search processing language",
    "eql",
    "monacor",
    "monarch",
    "vscode",
    "code",
    "syntax"
  ],
  "bugs": {
    "url": "https://github.com/elastic/monaco-esql/issues"
  },
  "homepage": "https://github.com/elastic/monaco-esql",
  "peerDependencies": {
    "monaco-editor": "*"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@chromatic-com/storybook": "4.1.3",
    "@monaco-editor/react": "4.7.0",
    "@storybook/addon-docs": "10.1.2",
    "@storybook/addon-onboarding": "10.1.2",
    "@storybook/addon-vitest": "10.1.2",
    "@storybook/react-vite": "10.1.2",
    "@types/react": "19.2.7",
    "@vitest/browser": "4.0.14",
    "@vitest/browser-playwright": "4.0.14",
    "@vitest/coverage-v8": "4.0.14",
    "monaco-editor": "0.55.1",
    "playwright": "1.57.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "storybook": "10.1.2",
    "typescript": "5.9.3",
    "vitest": "4.0.14"
  }
