{
  "name": "@elastic/monaco-esql",
  "version": "3.1.15",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "description": "Monaco editor Monarch language syntax definitions for ES|QL",
  "main": "lib/index.js",
  "files": [
    "NOTICE.txt",
    "LICENSE",
    "lib/"
  ],
  "scripts": {
    "format": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"no tests...\"",
    "clean": "npx rimraf@5.0.5 lib",
    "build:es2020": "tsc --project tsconfig.build.json --module commonjs --target es2020 --outDir lib",
    "build": "yarn build:es2020",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "release": "npx release-it --ci",
    "prepare": "husky"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elastic/monaco-esql.git"
  },
  "keywords": [
    "esql",
    "es|ql",
    "elastic",
    "search processing language",
    "eql",
    "monacor",
    "monarch",
    "vscode",
    "code",
    "syntax"
  ],
  "bugs": {
    "url": "https://github.com/elastic/monaco-esql/issues"
  },
  "homepage": "https://github.com/elastic/monaco-esql",
  "peerDependencies": {
    "monaco-editor": "*",
    "monaco-promql": "*"
  },
  "peerDependenciesMeta": {
    "monaco-promql": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.28.6",
    "@biomejs/biome": "2.3.11",
    "@chromatic-com/storybook": "5.0.0",
    "@eslint/js": "9.39.2",
    "@monaco-editor/react": "4.7.0",
    "@storybook/addon-docs": "10.2.6",
    "@storybook/addon-onboarding": "10.2.6",
    "@storybook/addon-vitest": "10.2.6",
    "@storybook/react-vite": "10.2.6",
    "@types/react": "19.2.9",
    "@vitest/browser": "4.0.18",
    "@vitest/browser-playwright": "4.0.18",
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "9.39.2",
    "eslint-plugin-react": "7.37.5",
    "globals": "17.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "monaco-editor": "0.55.1",
    "monaco-promql": "1.8.0",
    "playwright": "1.57.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "storybook": "10.2.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "vitest": "4.0.18"
  },
  "lint-staged": {
    "*.{js,ts,tsx,mjs}": [
      "eslint --fix"
    ]
  }
}
